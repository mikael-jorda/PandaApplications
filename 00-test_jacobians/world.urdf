<?xml version="1.0" ?>

<world name="demo_world" gravity="0.0 0.0 0.0">
<!-- <world name="demo_world" gravity="0.0 0.0 -9.81"> -->

	<robot name="PANDA">
		<model dir="../resources/00-test_jacobians" path="panda_arm_hand.urdf" name="panda_hand" />
		<origin xyz="0.0 -0.0 0.0" rpy="0 0 0" />
	</robot>

	<static_object name="Floor">
		<origin xyz="0.0 0.0 -0.05" rpy="0 0 0" />
		<collision>
	        <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
	        <geometry>
	            <box size="3.0 3.0 0.1" />
	        </geometry>
	    </collision>
	    <visual>
	        <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
	        <geometry>
	            <box size="3.0 3.0 0.1" />
	        </geometry>
	        <material name="material_gray">
		        <color rgba="0.6 0.6 0.6 1.0" />
	        </material>
	    </visual>
	</static_object>

	<light name="light1" type="directional">
		<position xyz="2.0 -2.0 2.0" />
		<lookat xyz="0.0 0.0 0.0" />
	</light>

	<light name="light2" type="directional">
		<position xyz="2.0 2.0 2.0" />
		<lookat xyz="0.0 0.0 0.0" />
	</light>

	<camera name="camera_fixed">
		<position xyz="1.0 1.0 0.5" />
		<vertical xyz="0.0 0.0 1.0" />
		<lookat xyz="0.0 0.0 0.5" />
	</camera>

	<!-- TODO: support stereo specification -->

</world>
